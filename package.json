{
  "name": "webui_discord_bot",
  "module": "index.ts",
  "type": "module",
  "author": "tkgling",
  "version": "1.0.0",
  "scripts": {
    "commit": "git-cz",
    "dev": "bun src/index.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint --ignore-path .gitignore '{src,test}/**/*.ts' --fix",
